{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    },
    {
      "name": "s3",
      "installMode": "lazy",
      "updateMode": "lazy",
      "resources": {
        "urls": [
          "https://s3.ap-southeast-1.amazonaws.com/penguin-stats-item-image/*.png"
        ]
      }
    }
  ]
}